      "source": [
        "def novocali(parola):\n",
        "  vocali = set(\"aeiou\")\n",
        "  for x in vocali:\n",
        "    if x in vocali:\n",
        "      parola = parola.replace(x,\"\")\n",
        "  return parola\n",
        "\n",
        "parola = \"anatra\"\n",
        "novocali(parola)"
      ],
